package Example;

public class Test2 {
	
	public static void main(String[] args) {
		
		String str = "   Hello World!     "; 
		
		System.out.println(str);
		System.out.println(str.length());
		
		String trimmed = str.trim();
		
		System.out.println(trimmed);
		System.out.println(trimmed.length());
	}

}
